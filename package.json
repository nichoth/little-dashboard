{
  "name": "little-dashboard",
  "description": "Dashboard UI",
  "main": "index.js",
  "version": "0.0.0",
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015"
          ]
        }
      ]
    ]
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build-js": "browserify -d example/main.js > public/bundle.js",
    "watch-js": "watchify example/main.js -o public/bundle.js -dv",
    "build-sass": "node-sass src/style/main.scss > public/style.css",
    "watch-sass": "node-sass --watch src/style/main.scss public/style.css",
    "dev": "npm run watch-js & npm run watch-sass & npm run server",
    "server": "node server.js",
    "demo": "browserify example/main.js > public/bundle.js && html-inline public/index.html | curl -sT- https://htmlb.in"
  },
  "author": "Nick Thomas <nichoth@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/nichoth/little-dashboard.git"
  },
  "license": "ISC",
  "peerDependencies": {},
  "devDependencies": {
    "babel-preset-es2015": "^6.0.15",
    "babelify": "^7.2.0",
    "browserify": "^12.0.1",
    "node-sass": "^3.3.3",
    "watchify": "^3.6.0"
  },
  "dependencies": {
    "@nichoth/router": "0.0.0",
    "@nichoth/state": "0.0.2",
    "ecstatic": "^1.1.0",
    "form-data-set": "^2.0.0",
    "level": "^1.3.0",
    "level-live-stream": "^1.4.11",
    "lodash.foreach": "^3.0.3",
    "lodash.map": "^3.1.4",
    "lodash.reduce": "^3.1.2",
    "main-loop": "^3.2.0",
    "multilevel": "^7.2.0",
    "normalize.css": "^3.0.3",
    "observ": "^0.2.0",
    "observ-array": "^3.2.1",
    "observ-struct": "^6.0.0",
    "observ-varhash": "^1.0.6",
    "routes": "^2.1.0",
    "shoe": "0.0.15",
    "slug": "https://github.com/nichoth/node-slug/tarball/master",
    "title-case": "^1.1.1",
    "vdom-components": "0.0.2",
    "vdom-form": "0.0.5",
    "vdom-form-field": "0.0.7",
    "vdom-input": "0.0.8",
    "vdom-sidebar": "0.0.6",
    "virtual-dom": "^2.1.1",
    "xtend": "^4.0.0"
  }
}
